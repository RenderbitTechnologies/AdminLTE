{
  "name": "renderbit-technologies/adminlte",
  "description": "AdminLTE integration for Laravel 5",
  "license": "MIT",
  "keywords": [
    "adminlte",
    " laravel",
    " bootstrap"
  ],
  "type": "library",
  "authors": [
    {
      "name": "Renderbit Technologies",
      "email": "contact@renderbit.com"
    }
  ],
  "require": {
    "php": ">=5.6",
    "illuminate/support": "~5.7.0"
  },
  "require-dev": {
    "orchestra/testbench": "~3.7.0",
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "RenderbitTechnologies\\AdminLTE\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "RenderbitTechnologies\\AdminLTE\\Tests\\": "tests"
    },
    "files": [
      "vendor/phpunit/phpunit/src/Framework/Assert/Functions.php"
    ]
  },
  "scripts": {
    "phpunit": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "RenderbitTechnologies\\AdminLTE\\ServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
